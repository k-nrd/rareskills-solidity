#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/knrd/code/courses/rareskills-solidity/week2/merkle-tree/node_modules/.pnpm/node-gyp-build@4.7.1/node_modules/node-gyp-build/node_modules:/home/knrd/code/courses/rareskills-solidity/week2/merkle-tree/node_modules/.pnpm/node-gyp-build@4.7.1/node_modules:/home/knrd/code/courses/rareskills-solidity/week2/merkle-tree/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/knrd/code/courses/rareskills-solidity/week2/merkle-tree/node_modules/.pnpm/node-gyp-build@4.7.1/node_modules/node-gyp-build/node_modules:/home/knrd/code/courses/rareskills-solidity/week2/merkle-tree/node_modules/.pnpm/node-gyp-build@4.7.1/node_modules:/home/knrd/code/courses/rareskills-solidity/week2/merkle-tree/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
